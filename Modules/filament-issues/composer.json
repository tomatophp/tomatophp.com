{
    "name": "tomatophp/filament-issues",
    "type": "library",
    "description": "Manage your github issues from your FilamentPHP panel and share issues with others",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tomatophp\\FilamentIssues\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentIssues\\FilamentIssuesServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "filament/filament": "^3.2",
        "tomatophp/console-helpers": "^1.1"
    }
}
